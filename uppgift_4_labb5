#include "stdafx.h"
#include "stdio.h"




int main()
{
	int i;
	int j;

	for (i = 0; i<5 ; i++){
		printf("*");
		for (j = 0; j < i; j++){
		printf("*");
		}
		printf("\n"); eller putchar('\n');
	}
	getchar();
	return 0;

}

